<div class="container text-center">

<h1>Ingrese Sus Datos</h1>
<%= form_with(model: @user, url: sessions_path, method: :post, local: true) do |f| %>

<div class="field">
  <%= f.label :email %><br />
  <%= f.text_field :email %>
</div>

<div class="field">
  <%= f.label :password %><br />
  <%= f.password_field :password %>
</div>

<div class="actions"><%= f.submit "Sign Up", :class => 'btn btn-info' %></div>
<% end %>

</div>
