<div class="container text-center">
<h1>Ingrese Los Datos</h1>

<%= form_with(model: @user) do |f| %>

<div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :email %><br />
  <%= f.text_field :email %>
</div>

<div class="field">
  <%= f.label :password %><br />
  <%= f.password_field :password %>
</div>

<div class="actions"><%= f.submit "Sign Up", :class => 'btn btn-success' %></div>
<% end %>

</div>
